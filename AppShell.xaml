<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ThrillRideTrackerMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ThrillRideTrackerMauiApp"
    xmlns:pages="clr-namespace:ThrillRideTrackerMauiApp.Pages"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:NewEntryPage}"
        Route="MainPage" />

</Shell>

